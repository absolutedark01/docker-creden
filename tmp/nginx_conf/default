                proxy_pass http://127.0.0.1:8124/face;
                proxy_http_version 1.1;
        }

        location /face/images/card {
                proxy_pass http://127.0.0.1:8124/card2;
                proxy_http_version 1.1;
        }

        location /edoc {
                proxy_pass http://127.0.0.1:8124/edoc;
                proxy_http_version 1.1;
        }

        location /gte {
                proxy_pass http://127.0.0.1:8080;
                proxy_http_version 1.1;
        }

        location /ekyc1 {
                ssi on;
                alias /home/creden/work/app/client/ekyc1;
        }

       location /socket.io {
                proxy_set_header Upgrade $http_upgrade;
                proxy_set_header Connection Upgrade;

                proxy_pass https://hjkl.ninja:5080/socket.io;
                proxy_http_version 1.1;
        }
